{
  "name": "renoun",
  "version": "4.0.0",
  "description": "The Content & Documentation SDK for React",
  "author": "Travis Arnold",
  "license": "AGPL-3.0-or-later",
  "repository": {
    "type": "git",
    "url": "https://github.com/souporserious/renoun",
    "directory": "packages/renoun"
  },
  "bugs": "https://github.com/souporserious/renoun/issues",
  "homepage": "https://renoun.dev",
  "keywords": [
    "markdown",
    "md",
    "mdx",
    "typescript",
    "ts",
    "tsx",
    "documentation",
    "docgen",
    "react",
    "design systems"
  ],
  "files": [
    "dist/*"
  ],
  "sideEffects": false,
  "bin": {
    "renoun": "./dist/cli/index.js"
  },
  "type": "module",
  "exports": {
    "./assets": {
      "types": "./dist/assets/index.d.ts",
      "import": "./dist/assets/index.js"
    },
    "./collections": {
      "types": "./dist/collections/index.d.ts",
      "import": "./dist/collections/index.js"
    },
    "./components": {
      "types": "./dist/components/index.d.ts",
      "import": "./dist/components/index.js"
    },
    "./components/*": {
      "types": "./dist/components/*.d.ts",
      "import": "./dist/components/*.js"
    },
    "./core": {
      "types": "./dist/core.d.ts",
      "import": "./dist/core.js"
    },
    "./project": {
      "types": "./dist/project/index.d.ts",
      "import": "./dist/project/index.js"
    }
  },
  "typesVersions": {
    "*": {
      "assets": [
        "./dist/assets/index.d.ts"
      ],
      "collections": [
        "./dist/collections/index.d.ts"
      ],
      "components": [
        "./dist/components/index.d.ts"
      ],
      "components/*": [
        "./dist/components/*.d.ts"
      ],
      "core": [
        "./dist/core.d.ts"
      ],
      "project": [
        "./dist/project/index.d.ts"
      ]
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc -w",
    "test": "vitest run",
    "postinstall": "pnpm build"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "devDependencies": {
    "@types/glob-parent": "5.1.3",
    "@types/mdx": "catalog:",
    "@types/react": "catalog:",
    "@types/react-dom": "catalog:",
    "@types/title": "^3.4.3",
    "@types/ws": "^8.5.12",
    "dedent": "^1.5.3",
    "react": "catalog:",
    "react-dom": "catalog:"
  },
  "dependencies": {
    "@mdx-js/mdx": "^3.0.1",
    "fast-glob": "^3.3.2",
    "glob-parent": "^6.0.2",
    "restyle": "catalog:",
    "server-only": "0.0.1",
    "shiki": "^1.17.7",
    "ts-morph": "^23.0.0",
    "ws": "^8.18.0"
  }
}
