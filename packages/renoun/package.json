{
  "name": "renoun",
  "version": "8.12.0",
  "description": "The Technical Content Toolkit for React",
  "author": "Travis Arnold",
  "license": "AGPL-3.0-or-later",
  "repository": {
    "type": "git",
    "url": "https://github.com/souporserious/renoun",
    "directory": "packages/renoun"
  },
  "bugs": "https://github.com/souporserious/renoun/issues",
  "homepage": "https://renoun.dev",
  "keywords": [
    "markdown",
    "md",
    "mdx",
    "typescript",
    "ts",
    "tsx",
    "documentation",
    "docgen",
    "react",
    "design systems"
  ],
  "files": [
    "dist/*"
  ],
  "sideEffects": false,
  "bin": {
    "renoun": "./dist/cli/index.js"
  },
  "type": "module",
  "imports": {
    "#fixtures/*": "./fixtures/*"
  },
  "exports": {
    "./assets": {
      "types": "./dist/assets/index.d.ts",
      "import": "./dist/assets/index.js",
      "default": "./dist/assets/index.js"
    },
    "./components": {
      "types": "./dist/components/index.d.ts",
      "import": "./dist/components/index.js",
      "default": "./dist/components/index.js"
    },
    "./components/*": {
      "types": "./dist/components/*.d.ts",
      "import": "./dist/components/*.js",
      "default": "./dist/components/*.js"
    },
    "./file-system": {
      "types": "./dist/file-system/index.d.ts",
      "import": "./dist/file-system/index.js",
      "default": "./dist/file-system/index.js"
    },
    "./mdx": {
      "types": "./dist/mdx/index.d.ts",
      "import": "./dist/mdx/index.js",
      "default": "./dist/mdx/index.js"
    },
    "./project": {
      "types": "./dist/project/client.d.ts",
      "import": "./dist/project/client.js",
      "default": "./dist/project/client.js"
    },
    "./server": {
      "types": "./dist/project/server.d.ts",
      "import": "./dist/project/server.js",
      "default": "./dist/project/server.js"
    },
    "./textmate": {
      "types": "./dist/textmate/index.d.ts",
      "import": "./dist/textmate/index.js",
      "default": "./dist/textmate/index.js"
    }
  },
  "typesVersions": {
    "*": {
      "assets": [
        "./dist/assets/index.d.ts"
      ],
      "components": [
        "./dist/components/index.d.ts"
      ],
      "components/*": [
        "./dist/components/*.d.ts"
      ],
      "file-system": [
        "./dist/file-system/index.d.ts"
      ],
      "mdx": [
        "./dist/mdx/index.d.ts"
      ],
      "project": [
        "./dist/project/client.d.ts"
      ],
      "server": [
        "./dist/project/server.d.ts"
      ],
      "textmate": [
        "./dist/textmate/index.d.ts"
      ]
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc -w",
    "textmate": "tsx ./scripts/textmate.ts",
    "test": "vitest --typecheck"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "devDependencies": {
    "@mdx-js/rollup": "^3.1.0",
    "@types/react": "catalog:",
    "@types/react-dom": "catalog:",
    "@types/title": "^3.4.3",
    "@types/ws": "^8.18.0",
    "dedent": "^1.5.3",
    "react": "catalog:",
    "react-dom": "catalog:",
    "tm-grammars": "^1.23.6",
    "tm-themes": "^1.10.2",
    "valibot": "catalog:",
    "zod": "catalog:"
  },
  "dependencies": {
    "@mdx-js/mdx": "^3.1.0",
    "@renoun/mdx": "workspace:*",
    "@shikijs/langs": "^3.2.1",
    "@shikijs/themes": "^3.2.1",
    "ignore": "^7.0.3",
    "minimatch": "^10.0.1",
    "oniguruma-to-es": "^4.1.0",
    "restyle": "catalog:",
    "ts-morph": "catalog:",
    "vscode-textmate": "^9.2.0",
    "ws": "^8.18.1"
  }
}
