{
  "name": "@renoun/mdx",
  "version": "3.3.0",
  "description": "MDX plugins and utilities for renoun",
  "author": {
    "name": "souporserious",
    "email": "support@souporserious.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/souporserious/renoun.git",
    "directory": "packages/mdx-plugins"
  },
  "bugs": "https://github.com/souporserious/renoun/issues",
  "homepage": "https://renoun.dev/mdx-plugins",
  "keywords": [
    "markdown",
    "md",
    "mdx",
    "documentation"
  ],
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/*"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsc",
    "dev": "tsc -w",
    "test": "vitest --typecheck"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./rehype": {
      "types": "./dist/rehype/index.d.ts",
      "import": "./dist/rehype/index.js",
      "default": "./dist/rehype/index.js"
    },
    "./rehype/*": {
      "types": "./dist/rehype/*.d.ts",
      "import": "./dist/rehype/*.js",
      "default": "./dist/rehype/*.js"
    },
    "./remark": {
      "types": "./dist/remark/index.d.ts",
      "import": "./dist/remark/index.js",
      "default": "./dist/remark/index.js"
    },
    "./remark/*": {
      "types": "./dist/remark/*.d.ts",
      "import": "./dist/remark/*.js",
      "default": "./dist/remark/*.js"
    },
    "./utils": {
      "types": "./dist/utils/index.d.ts",
      "import": "./dist/utils/index.js",
      "default": "./dist/utils/index.js"
    }
  },
  "typesVersions": {
    "*": {
      "rehype": [
        "./dist/rehype/index.d.ts"
      ],
      "rehype/*": [
        "./dist/rehype/*.d.ts"
      ],
      "remark": [
        "./dist/remark/index.d.ts"
      ],
      "remark/*": [
        "./dist/remark/*.d.ts"
      ],
      "utils": [
        "./dist/utils/index.d.ts"
      ]
    }
  },
  "peerDependencies": {
    "react": ">=18.0.0"
  },
  "devDependencies": {
    "@types/estree": "1.0.8",
    "@types/hast": "catalog:",
    "@types/mdast": "4.0.4",
    "@types/react": "catalog:",
    "@types/unist": "3.0.3",
    "react": "catalog:",
    "rehype-parse": "9.0.1",
    "vfile": "6.0.3",
    "vfile-message": "4.0.3"
  },
  "dependencies": {
    "@mdx-js/mdx": "3.1.1",
    "@types/mdx": "catalog:",
    "estree-util-value-to-estree": "3.4.0",
    "hast-util-to-jsx-runtime": "2.3.6",
    "mdast-util-mdx": "3.0.0",
    "mdast-util-to-string": "4.0.0",
    "rehype-unwrap-images": "1.0.0",
    "remark-gfm": "4.0.1",
    "remark-github": "12.0.0",
    "remark-mdx": "3.1.1",
    "remark-parse": "11.0.0",
    "remark-rehype": "11.1.2",
    "remark-smartypants": "3.0.2",
    "unified": "catalog:",
    "unist-util-mdx-define": "1.1.2",
    "unist-util-visit": "5.0.0",
    "unist-util-visit-parents": "6.0.1"
  }
}
